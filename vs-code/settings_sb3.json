{
    "editor.tabSize": 2,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wordWrap": "on",
    "files.eol": "\n",
    "files.encoding": "utf8",
    "files.maxMemoryForLargeFilesMB": 2048,
    //"terminal.integrated.shell.linux": "/usr/bin/zsh",
    "window.openFilesInNewWindow": "on",
    "window.restoreWindows": "all",
    "window.newWindowDimensions": "inherit",
    "workbench.colorCustomizations": {
        "editorError.foreground": "#ffa500",
        "editorWarning.foreground": "#ffa500"
    },
    "workbench.colorTheme": "Plastic",
    "workbench.editor.enablePreview": false,
    "workbench.iconTheme": "vscode-icons",
    "breadcrumbs.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "vsicons.projectDetection.disableDetect": true,
    "explorer.autoReveal": false,
    "editor.suggestSelection": "first",
    // git
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    // python
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    // java world
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.home": "C:\\Java\\jdk-11.0.11",
    "maven.executable.options": "",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    //"editor.fontFamily": "'Meslo LGS NF', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.mouseWheelZoom": true,
    "sonarlint.ls.javaHome": "C:\\Java\\jdk-11.0.11",
    "auto-close-tag.SublimeText3Mode": true,
    "java.semanticHighlighting.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "java.project.importHint": false,
    "aws.telemetry": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "remote.restoreForwardedPorts": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "explorer.confirmDelete": false,
    "tabnine.experimentalAutoImports": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "sonarlint.pathToNodeExecutable": "C:\\Program Files\\nodejs\\node.exe",
    "python.languageServer": "Default",
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.editor.wrapTabs": true,
    "workbench.editor.decorations.badges": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.tabSizing": "shrink",
    "java.codeGeneration.toString.codeStyle": "STRING_BUILDER",
    "redhat.telemetry.enabled": false,
    "editor.fontFamily": "'JetBrains Mono', 'Meslo LG M', Consolas, 'Courier New', monospace",
    "workbench.startupEditor": "newUntitledFile",
    "windowsTerminal.channel": "preview",
    "terminal.integrated.fontFamily": "'Meslo LG M', Consolas, 'Courier New', monospace",
    "sonarlint.rules": {
        "java:S1135": {
            "level": "off"
        },
        "java:S125": {
            "level": "off"
        }
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "java.project.importOnFirstTimeStartup": "automatic",
    "java.refactor.renameFromFileExplorer": "autoApply",
    "code-runner.executorMap": {
        "python": "python3",
    },
    "terminal.integrated.copyOnSelection": true,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "code-runner.enableAppInsights": false,
    "terminal.integrated.tabs.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "java.format.enabled": false,
    "editor.copyWithSyntaxHighlighting": false,
    "docker.showStartPage": false,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.schemas": {},
    "git.autofetch": true,
    "gitlens.advanced.messages": {
        "suppressCreatePullRequestPrompt": true,
        "suppressRebaseSwitchToTextWarning": true
    },
    "json.schemas": [],
    "gitlens.advanced.fileHistoryFollowsRenames": true,
    "gitlens.changes.locations": [
        "overview",
        "gutter"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "nginx-conf-hint.syntax": "sublime",
    "markdown.extension.toc.updateOnSave": false,
    "git.confirmSync": false,
    "aws.profile": "profile:default",
    "aws.onDefaultRegionMissing": "add",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    "svelte.enable-ts-plugin": true,
    "snyk.yesWelcomeNotification": false,
    "snyk.yesCrashReport": false,
    "snyk.yesTelemetry": false,
    "sonarlint.disableTelemetry": true,
}
